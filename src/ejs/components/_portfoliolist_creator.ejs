<% if (user_id) { %>
<div class="portfolio-list">
  <div class="panels panels--works" id="panels">
    <div class="panels__body js-masonry" data-masonry-options='{"columnWidth":220,"itemSelector":".panel","gutter":20}'>
      <% for(var i = 0; i < portfolios.length; i++) {%>
      <% if (portfolios[i].user_id == user_id ) { %>
      <div class="panel">
        <div class="panel__body">
          <img data-toggle="modal"
            data-target="#modal"
            data-toggle="modal"
            data-modal-portfolio="<%= this.SITE_ROOT %>img/portfolios/<%= portfolios[i]['id'] %>/<%= portfolios[i]['image_url'] %>"
            data-modal-creator="<%= this.SITE_ROOT %>img/creator/<%= portfolios[i]['id'] %>.png"
            <% if(portfolios[i]['product_id'] != null && portfolios[i]['product_id'] != "" ){ %>
            data-modal-product="<%= this.SITE_ROOT %>img/products/<%= portfolios[i]['product_id'] %>.png"
            <% } %>
            data-modal-name="<%=portfolios[i]['user_name']%>"
            data-modal-name-en="<%=portfolios[i]['user_name_en']%>"
            data-modal-tag-name="<%= portfolios[i]['tag_name']%>"
            src="<%= this.SITE_ROOT %>img/thumbnails/<%= portfolios[i]['id'] %>/<%= portfolios[i]['thumbnail_url'] %>"
            alt="<%= portfolios[i]['product_name']%>„ÅÆ<%= portfolios[i]['tag_name']%>"
          >
          <div class="panel__category">
            <% if( portfolios[i]['product_id']!=null && portfolios[i]['product_id'] !== '' ){ %>
            <a href="<%= this.SITE_ROOT %>categories/products/<%= portfolios[i]['product_id'] %>.html"><span class="icon icon__3"></span> <%= portfolios[i]['product_name']%></a>
            <br>
            <% } %>
            <a href="<%= this.SITE_ROOT %>categories/tags/<%= portfolios[i]['tag_id'] %>.html"><span class="icon icon__3"></span> <%= portfolios[i]['tag_name']%></a>
          </div>
        </div>
      </div>
      <% } %>
      <% } %>
    </div>
  </div>
</div>
<% } %>
