    "gulp-concat": "^2.5.2",
    "gulp-ejs": "^1.1.0",
    "gulp-html": "^0.4.2",
    "gulp-minify-css": "^1.1.1",
    "gulp-minify-html": "^1.0.2",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.0",
    "gulp-sass": "^2.0.0",
    "gulp-scss-lint": "^0.1.12",
    "gulp-uglify": "^1.2.0",
    "gulp-watch": "^4.2.0",
    "mkdirp": "^0.5.1"
  },
  "dependencies": {
    "jquery": "^2.1.4",
    "jquery-lazyload": "^1.9.5",
